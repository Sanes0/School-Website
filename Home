<script>
  const div11 = document.querySelector('.div-11');
  const div12 = document.querySelector('.div-12');
  const div13 = document.querySelector('.div-13');
  const contentContainer = document.querySelector('.content-container');


  div11.addEventListener('mouseenter', () => {
    div11.style.backgroundColor = '#fff';
    div11.style.transition = 'background-color 0.3s';
    div11.style.color = "#001767";
  });


  div11.addEventListener('mouseleave', () => {
    div11.style.backgroundColor = 'transparent';
    div11.style.color = 'white';
  });


  div12.addEventListener('mouseenter', () => {
    div12.style.backgroundColor = '#fff';
    div12.style.transition = 'background-color 0.3s';
    div12.style.color = "#001767";
  });


  div12.addEventListener('mouseleave', () => {
    div12.style.backgroundColor = 'transparent';
    div12.style.color = 'white';
  });


  div13.addEventListener('mouseenter', () => {
    div13.style.backgroundColor = '#fff';
    div13.style.transition = 'background-color 0.3s';
    div13.style.color = "#001767";
  });


  div13.addEventListener('mouseleave', () => {
    div13.style.backgroundColor = 'transparent';
    div13.style.color = 'white';
  });


  div11.addEventListener('click', () => {
    window.location.href = 'C:/Users/sirfe/OneDrive/Documents/School website/index.html';
  });


  div12.addEventListener('click', () => {
    window.location.href = 'C:/Users/sirfe/OneDrive/Documents/School website/index2.html';
  });


  div13.addEventListener('click', () => {
    window.location.href = 'path/to/contact.html';
  });
</script>
 
